        "contact_facebook": "YoloCompany",
        "educational_level": "KULIAH",
        "cloudinary_id": "belajar_probation/57fbb73d6b9f8dbb195acb8d5d47cd9e_gelp9y",
        "slug": "beasiswa-prestasi-yolo-co-id-3-0",
        "__v": 0
    }
}
