{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "camelcase":"off",
        "consistent-return":"off",
        "max-len":"off",
        "no-return-await":"off",
        "no-restricted-syntax":"off",
        "no-await-in-loop":"off",
        "no-plusplus":"off",
        "eqeqeq":"off",
        "no-underscore-dangle":"off",
        "no-useless-escape":"off",
        "block-scoped-var":"off",
        "no-var":"off",
        "vars-on-top":"off"
        
    }
}
